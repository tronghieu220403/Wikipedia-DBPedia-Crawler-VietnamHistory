: [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Operation_Black_Lion" } ] } ,  "http://dbpedia.org/resource/Operation_Fa_Ngum" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Operation_Black_Lion" } ] } ,  "http://dbpedia.org/resource/Wilbur_%22Will%22_Green" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Operation_Black_Lion" } ] } ,  "http://dbpedia.org/resource/List_of_military_operations" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Operation_Black_Lion" } ] } ,  "http://dbpedia.org/resource/Operation_Black_Lion" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://schema.org/Event" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/SocietalEvent" } ,      { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/Event" } ,      { "type" : "uri", "value" : "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/MilitaryConflict" } ,      { "type" : "uri", "value" : "http://www.wikidata.org/entity/Q1656682" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Operation Black Lion" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Operation Black Lion (15 June \u2013 19 October 1972) was a Royal Lao Government counter-offensive against a People's Army of Vietnam thrust that cut the Kingdom of Laos in two at Khong Sedone during May 1972. Two regiments of Royalist military irregulars retaliated on 15 June 1972, attacking the Communist 39th Regiment in an air assault while Royal Lao Air Force tactical air strikes hammered the 39th. During the next month, the 39th Regiment would suffer an estimated 360 casualties and be rendered ineffective for attacks. On 18 July, they retreated, leaving a rear guard to be overrun." , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Operation_Left_Jab" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "Operation Black Lion" , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/depiction" : [ { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/AC-130.jpeg" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Laotian_Civil_War" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Operation Black Lion (15 June \u2013 19 October 1972) was a Royal Lao Government counter-offensive against a People's Army of Vietnam thrust that cut the Kingdom of Laos in two at Khong Sedone during May 1972. Two regiments of Royalist military irregulars retaliated on 15 June 1972, attacking the Communist 39th Regiment in an air assault while Royal Lao Air Force tactical air strikes hammered the 39th. During the next month, the 39th Regiment would suffer an estimated 360 casualties and be rendered ineffective for attacks. On 18 July, they retreated, leaving a rear guard to be overrun. Throughout the following months, the Royalists pushed eastward along Route 23 towards Vietnam to capture Laongam. Then, on the nights of 14/15 and 18/19 October 1972, Communist PT-76 tanks struck four Royalist Thai mercenary battalions holding Laongam. All the Thais except the regimental staff abandoned the town on 19 October; however, an AC-130 Spectre gunship repelled the Communist attack. Two Thai battalions returned to form a weak defense." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Military_irregular" } ,      { "type" : "u
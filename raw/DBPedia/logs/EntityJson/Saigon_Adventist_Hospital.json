{  "http://dbpedia.org/resource/Saigon_Adventist_Hospital" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Artifact100021939" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Object100002684" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/YagoGeoEntity" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Whole100003553" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/building" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/PhysicalEntity100001930" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/Hospital" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/MedicalBuilding103739518" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatHospitalsAffiliatedWithTheSeventh-dayAdventistChurch" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Building102913152" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Hospital103540595" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/YagoPermanentlyLocatedEntity" } ,      { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Structure104341686" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Saigon Adventist Hospital" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Saigon Adventist Hospital or US Army 3rd Field Hospital was an Adventist hospital in Ho Chi Minh City. It was a private hospital, formerly operated by the United States Army before being given to the Seventh-day Adventist Church. The hospital was operated by the Loma Linda University School of Medicine and performed the only open heart surgery operation in Vietnam at the time. The hospital was a former mansion converted to facilitate 38 hospital beds." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "Saigon Adventist Hospital" , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/depiction" : [ { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Saigon_Adventist_Hospital_1955.png" } ,      { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/NARA_photo_111-CCV-1-CC68096.jpg" } ,      { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Vietnam-021-3rd-Field-front.jpg" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Former_Seventh-day_Adventist_institutions" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Hospitals_affiliated_with_the_Seventh-day_Adventist_Church" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1975_disestablishments_in_Vietnam" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Saigon Adventist Hospital or US Army 3rd Field Hospital was an Adventist hospital in Ho Chi Minh City. It was a private hospital, formerly operated by the United States Army before being given to the Seventh-day Adventist Church. The hospital was operated by the Loma Linda University School of Medicine and performed the only open heart surgery operation in Vietnam at the time. The hospital was a former mansion converted to facilitate 38 hospital beds. In March 1973, the hospital was moved from the location at the Phu Nhuan crossroads to the former US Army 3rd Field Hospital. The move was to be temporary until construction of a new hospital at another location was completed. More than 410 employees and church workers escaped before the Fall of Saigon. However, thousands of members and many pastors and teachers and other employees remained behind in Vietnam. The remaining members reorganised the work in Vietnam. Some of these members lost their lives while some were forced into re-education camps. Many of the workers could not move or travel from one area to another without permission. Most of the churches were shut down, and all the schools were closed." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/List_of_Seventh-day_Adventist_hospitals" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Ho_Chi_Minh_City" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Former_Seventh-day_Adventist_institutions" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/United_States_Army" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/File:Vietnam-021-3rd-Field-front.jpg" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/File:NARA_photo_111-CCV-1-CC68096.jpg" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Loma_Linda_University" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Hospitals_affiliated_with_the_Seventh-day_Adventist_Church" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Seventh-day_Adventist_Church" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1975_disestablishments_in_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Fall_of_Saigon" } ] ,    "http://dbpedia.org/property/wikiPageUsesTemplate" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Coord_missing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:SeventhdayAdventist-stub" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Asia-hospital-stub" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Authority_control" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resourc
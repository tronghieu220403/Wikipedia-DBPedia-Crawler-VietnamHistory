{  "http://dbpedia.org/resource/Index_of_Buddhism-related_articles" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Buddhism_in_Bulgaria" } ] } ,  "http://en.wikipedia.org/wiki/Buddhism_in_Bulgaria" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Buddhism_in_Bulgaria" } ] } ,  "http://dbpedia.org/resource/Outline_of_Bulgaria" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Buddhism_in_Bulgaria" } ] } ,  "http://dbpedia.org/resource/Buddhism_in_Bulgaria" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/ontology/EthnicGroup" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Buddhism in Bulgaria" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Buddhism is a small minority religion in Bulgaria, with about a thousand practitioners. The Vietnamese community in Bulgaria traditionally practices Mahayana Buddhism alongside Ancestor Worship, but the population of this community, which mostly hails from North Vietnam, has declined from tens of thousands before 1990 to around 1,000 now. Some Buddhist believers in Bulgaria are of Chinese descent. A small number of native Bulgarians are converting to Theravada and Tibetan Buddhism." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/homepage" : [ { "type" : "uri", "value" : "http://www.diamondway.bg" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buddhism_by_country" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Religion_in_Bulgaria" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buddhism_in_Europe" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Buddhism is a small minority religion in Bulgaria, with about a thousand practitioners. The Vietnamese community in Bulgaria traditionally practices Mahayana Buddhism alongside Ancestor Worship, but the population of this community, which mostly hails from North Vietnam, has declined from tens of thousands before 1990 to around 1,000 now. Some Buddhist believers in Bulgaria are of Chinese descent. A small number of native Bulgarians are converting to Theravada and Tibetan Buddhism. There is an officially recognized Bulgarian Buddhist Karma Kagyu organization, which is a part of Diamond Way Buddhism led by Karmapa Trinley Thaye Dorje. They have several centers spread over the country. Shechen has a mountain retreat center not far away from Sofia; access is granted to members, and it is not generally open to the public." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Bulgaria" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Shechen" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Theravada" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Sofia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Tibetan_Buddhism" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Religion_in_Bulgaria" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/North_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Karma_Kagyu" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Han_Chinese" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Ancestor_Worship" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buddhism_by_country" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buddhism_in_Europe" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Religion_in_Bulgaria" } ,
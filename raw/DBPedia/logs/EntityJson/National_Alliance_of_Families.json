{  "http://en.wikipedia.org/wiki/National_Alliance_of_Families" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/National_Alliance_of_Families" } ] } ,  "http://dbpedia.org/resource/National_League_of_POW/MIA_Families" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/National_Alliance_of_Families" } ] } ,  "http://dbpedia.org/resource/United_States_Senate_Select_Committee_on_POW/MIA_Affairs" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/National_Alliance_of_Families" } ] } ,  "http://dbpedia.org/resource/National_Alliance_of_Families" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/class/yago/PsychologicalFeature100023100" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Cognition100023271" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/Organisation" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Issue105814650" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Abstraction100002137" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatVietnamWarPOW/MIAIssues" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Content105809192" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "National Alliance of Families" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "The National Alliance of Families for the Return of America's Missing Servicemen is an American organization founded in 1990. Its goal is to resolve the fates of any unreturned U.S. prisoners of war or missing in action from World War II on forward, and to gain the return of any live prisoners." , "lang" : "en" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Vietnam_War_POW/MIA_issues" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:POW/MIA_advocacy" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "The National Alliance of Families for the Return of America's Missing Servicemen is an American organization founded in 1990. Its goal is to resolve the fates of any unreturned U.S. prisoners of war or missing in action from World War II on forward, and to gain the return of any live prisoners. The group is a 1980s splinter from the older National League of Families, created by members who were dissatisfied with Ann Mills Griffiths' leadership. Compared to the older group, the National Alliance takes a more activist, radical stance, especially with regards towards the Vietnam War POW/MIA issue and belief in the existence of live prisoners in Southeast Asia. The chair and co-founder of the group is Dolores Apodaca Alfond, whose brother was shot down in 1967 during the Vietnam War. The group was visible during the United States Senate Select Committee on POW/MIA Affairs hearings of the early 1990s, but disagreed with the committee's findings that there was no compelling evidence of any live prisoners in Southeast Asia. More than sixteen hundred U.S. servicemen are still listed as missing in action in Southeast Asia. The National Alliance of Families has also championed the case of Gulf War missing airman Scott Speicher, and also U.S. Prisoners of War or Missing in Action statused service members in the current Iraq and Afghanistan conflicts as well." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Missing_in_action" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/United_States_Senate_Select_Committee_on_POW/MIA_Affairs" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_War_POW/MIA_issue" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Gulf_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Prisoners_of_war" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/World_War_II" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Vietnam_War_POW/MIA_issues" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/National_League_of_Families" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:POW/MIA_advocacy" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Scott_Speicher" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Schism_(organizational)" } ] ,    "http://dbpedia.org/property/wikiPageUsesTemplate" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Reflist" } ] ,    "http://dbpedia.org/ontology/wikiPageRevisionID" : [ { "type" : "literal", "value" : 1091002603 , "datatype" : "http://www.w3.org/2001/XMLSchema#integer" } ] ,    "http://dbpedia.org/ontology/wikiPageExternalLink" : [ { "type" : "uri", "value" : "http://www.nationalalliance.org/" } ] ,    "http://dbpedia.org/ontology/wikiPageLength" : [ { "type" : "literal", "value" : "3106" , "datatype" : "http://www.w3.org/2001/XMLSchema#nonNegativeInteger" } ] ,    "http://dbpedia.org/ontology/wikiPageID" : [ { "type" : "literal", "value" : 17846878 , "datatype" : "http://www.w3.org/2001/XMLSchema#integer" } ] ,    "http://www.w3.org/2002/07/owl#sameAs" : [ { "type" : "uri", "value" : "https://global.dbpedia.org/id/4sGeD" } ,      { "type" : "uri", "value" : "http://www.wikidata.org/entity/Q6970311" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/National_Alliance_of_Families" } ,      { "type" : "uri", "value" : "http://rdf.freebase.com/ns/m.047bhr7" } ,      { "type" : "uri", "value" : "http://yago-knowledge.org/resource/National_Alliance_o
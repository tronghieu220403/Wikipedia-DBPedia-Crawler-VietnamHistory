{  "http://en.wikipedia.org/wiki/Special_Service_Medal_(South_Vietnam)" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Special_Service_Medal_(South_Vietnam)" } ] } ,  "http://dbpedia.org/resource/Orders,_decorations,_and_medals_of_South_Vietnam" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Special_Service_Medal_(South_Vietnam)" } ] } ,  "http://dbpedia.org/resource/Meritorious_Service_Medal_(Vietnam)" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Special_Service_Medal_(South_Vietnam)" } ] ,    "http://dbpedia.org/property/lower" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Special_Service_Medal_(South_Vietnam)" } ] } ,  "http://dbpedia.org/resource/Awards_and_decorations_of_the_United_States_Armed_Forces" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Special_Service_Medal_(South_Vietnam)" } ] } ,  "http://dbpedia.org/resource/Special_Service_Medal_(South_Vietnam)" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/ontology/Award" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Communication100033020" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatMilitaryAwardsAndDecorationsOfVietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Signal106791372" } ,      { "type" : "uri", "value" : "http://www.wikidata.org/entity/Q618779" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Award106696483" } ,      { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Symbol106806469" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Abstraction100002137" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Special Service Medal (South Vietnam)" , "lang" : "en" } ,      { "type" : "literal", "value" : "\u041C\u0435\u0434\u0430\u043B\u044C \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u044B\u0445 \u0437\u0430\u0441\u043B\u0443\u0433 (\u042E\u0436\u043D\u044B\u0439 \u0412\u044C\u0435\u0442\u043D\u0430\u043C)" , "lang" : "ru" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "\u041C\u0435\u0434\u0430\u043B\u044C \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u044B\u0445 \u0437\u0430\u0441\u043B\u0443\u0433 \u2014 \u0432\u043E\u0435\u043D\u043D\u0430\u044F \u043D\u0430\u0433\u0440\u0430\u0434\u0430 \u042E\u0436\u043D\u043E\u0433\u043E \u0412\u044C\u0435\u0442\u043D\u0430\u043C\u0430." , "lang" : "ru" } ,      { "type" : "literal", "value" : "The Special Service Medal (Vietnamese: Bi\u1EC7t-C\u00F4ng B\u1ED9i-Tinh) was a decoration of South Vietnam which was issued between the years of 1950 and 1974. The decoration was awarded to any military service member who performed an act of outstanding meritorious service to the Vietnamese State. The Special Service Medal was also awarded to members of foreign militaries and was presented frequently to members of the United States Armed Forces during the years of the Vietnam War." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "Vietnam Special Service Medal" , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/depiction" : [ { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Special_Service_Medal_(South_Vietnam).png" } ,      { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Vietnam_Special_Service_Medal_ribbon.svg" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_awards_and_decorations_of_Vietnam" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "The Special Service Medal (Vietnamese: Bi\u1EC7t-C\u00F4ng B\u1ED9i-Tinh) was a decoration of South Vietnam which was issued between the years of 1950 and 1974. The decoration was awarded to any military service member who performed an act of outstanding meritorious service to the Vietnamese State. The Special Service Medal was also awarded to members of foreign militaries and was presented frequently to members of the United States Armed Forces during the years of the Vietnam War. The U.S. military considered the Special Service Medal to be the equivalent of the Bronze Star. When awarded to U.S. personnel, the decoration was permitted for wear on U.S. uniforms after all United States military awards. In 1975, with the fall of Saigon, the South Vietnamese military effectively ceased to exist and the Special Service Medal became obsolete. Today, it is only available by private purchase through military insignia dealers." , "lang" : "en" } ,      { "type" : "literal", "value" : "\u041C\u0435\u0434\u0430\u043B\u044C \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u044B\u0445 \u0437\u0430\u0441\u043B\u0443\u0433 \u2014 \u0432\u043E\u0435\u043D\u043D\u0430\u044F \u043D\u0430\u0433\u0440\u0430\u0434\u0430 \u042E\u0436\u043D\u043E\u0433\u043E \u0412\u044C\u0435\u0442\u043D\u0430\u043C\u0430." , "lang" : "ru" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Gallantry_Cross_(Vietnam)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Awards_and_decorations_of_the_United_States_military" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Bronze_Star_Medal" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/List_of_military_decorations" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Saigon" } ,      { "type" : "ur
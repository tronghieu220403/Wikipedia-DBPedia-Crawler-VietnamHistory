{  "http://dbpedia.org/resource/Wound_Medal_(Vietnam)" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Symbol106806469" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/Award" } ,      { "type" : "uri", "value" : "http://www.wikidata.org/entity/Q618779" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Communication100033020" } ,      { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Award106696483" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Signal106791372" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatMilitaryAwardsAndDecorationsOfVietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Abstraction100002137" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "\u041C\u0435\u0434\u0430\u043B\u044C \u00AB\u0417\u0430 \u0440\u0430\u043D\u0435\u043D\u0438\u0435\u00BB (\u042E\u0436\u043D\u044B\u0439 \u0412\u044C\u0435\u0442\u043D\u0430\u043C)" , "lang" : "ru" } ,      { "type" : "literal", "value" : "Wound Medal (Vietnam)" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "\u041C\u0435\u0434\u0430\u043B\u044C \u00AB\u0417\u0430 \u0440\u0430\u043D\u0435\u043D\u0438\u0435\u00BB (\u0432\u044C\u0435\u0442\u043D. B\u1EA3o qu\u1ED1c Hu\u00E2n ch\u01B0\u01A1ng) \u2014 \u0432\u043E\u0435\u043D\u043D\u0430\u044F \u043D\u0430\u0433\u0440\u0430\u0434\u0430 \u042E\u0436\u043D\u043E\u0433\u043E \u0412\u044C\u0435\u0442\u043D\u0430\u043C\u0430." , "lang" : "ru" } ,      { "type" : "literal", "value" : "The Republic of Vietnam Wound Medal (Vietnamese: Chi\u1EBFn-Th\u01B0\u01A1ng B\u1ED9i-Tinh) was a military decoration of South Vietnam first created in 1953. The medal was the South Vietnamese equivalent of the United States military's Purple Heart, and was awarded to any personnel of the South Vietnamese military who, while engaged in armed combat with enemies of the Republic of Vietnam, were either wounded or killed in action. With the fall of South Vietnam in 1975, the Wound Medal became obsolete." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "Wound Medal" , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/depiction" : [ { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Vietnam_Wound_Medal.png" } ,      { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Vietnam_Wound_Medal_ribbon.svg" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_awards_and_decorations_of_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Wound_decorations" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "The Republic of Vietnam Wound Medal (Vietnamese: Chi\u1EBFn-Th\u01B0\u01A1ng B\u1ED9i-Tinh) was a military decoration of South Vietnam first created in 1953. The medal was the South Vietnamese equivalent of the United States military's Purple Heart, and was awarded to any personnel of the South Vietnamese military who, while engaged in armed combat with enemies of the Republic of Vietnam, were either wounded or killed in action. During the Vietnam War, the Wound Medal was often issued to Army of the Republic of Vietnam (ARVN) personnel for combat wounds received fighting the forces of North Vietnam or the Vietcong. The medal was seldom bestowed upon members of allied militaries such as the United States military and then only if the allied soldier in question was attached and under the direct command of a South Vietnamese unit. For service members of the United States military, the Wound Medal is not authorized for wear on a military uniform. With the fall of South Vietnam in 1975, the Wound Medal became obsolete." , "lang" : "en" } ,      { "type" : "literal", "value" : "\u041C\u0435\u0434\u0430\u043B\u044C \u00AB\u0417\u0430 \u0440\u0430\u043D\u0435\u043D\u0438\u0435\u00BB (\u0432\u044C\u0435\u0442\u043D. B\u1EA3o qu\u1ED1c Hu\u00E2n ch\u01B0\u01A1ng) \u2014 \u0432\u043E\u0435\u043D\u043D\u0430\u044F \u043D\u0430\u0433\u0440\u0430\u0434\u0430 \u042E\u0436\u043D\u043E\u0433\u043E \u0412\u044C\u0435\u0442\u043D\u0430\u043C\u0430." , "lang" : "ru" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/United_States_military" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_awards_and_decorations_of_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Wound_decorations" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Army_of_the_Republic_of_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietcong" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Purple_Heart" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/North_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/South_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Armed_Forces_Honor_Medal" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Loyalty_Medal" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/List_of_military_decorations" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Medal_for_the_War_Wounded" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/File:Vietnam_Wound_Medal_ribbon.svg" } ] ,    "http://dbpedia.org/property/wikiPageUsesTemplate" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Infobox_Military_Award" } ,      { "type" : "uri", "value" : "http://dbp
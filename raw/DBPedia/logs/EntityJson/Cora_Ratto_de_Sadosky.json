{  "http://en.wikipedia.org/wiki/Cora_Ratto_de_Sadosky" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Cora_Ratto_de_Sadosky" } ] } ,  "http://dbpedia.org/resource/Cora_Ratto_de_Sadosky" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Cora Ratto de Sadosky" , "lang" : "eu" } ,      { "type" : "literal", "value" : "Cora Ratto de Sadosky" , "lang" : "en" } ,      { "type" : "literal", "value" : "Cora Ratto de Sadosky" , "lang" : "de" } ,      { "type" : "literal", "value" : "Cora Ratto" , "lang" : "es" } ,      { "type" : "literal", "value" : "Cora Ratto de Sadosky" , "lang" : "fr" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Cora Ratto de Sadosky n\u00E9e le 3 janvier 1912 en Argentine et morte le 2 janvier 1981 est une math\u00E9maticienne et militante des droits de l'homme et des droits des femmes argentine." , "lang" : "fr" } ,      { "type" : "literal", "value" : "Corina Elo\u00EDsa Ratto (Buenos Aires, Argentina, 1912ko urtarrilaren 3a - 1981eko urtarrilaren 2a) matematikaria, giza eskubideen eta Argentinako emakumeen eskubideen aldeko aktibista izan zen." , "lang" : "eu" } ,      { "type" : "literal", "value" : "Cora Eloisa Ratto de Sadosky (* 3. Januar 1912 in Argentinien; \u2020 2. Januar 1981 in Barcelona) war eine argentinische Mathematikerin, Hochschullehrerin und Menschenrechtsaktivistin." , "lang" : "de" } ,      { "type" : "literal", "value" : "Corina (Cora) Elo\u00EDsa Ratto de Sadosky (aka Cora Ratto, 1912\u20131981) was an Argentine mathematician, educator and militant activist in support of human and women's rights in Argentina and beyond. She played an important part in the Argentine University Federation supporting republican interests during the Spanish Civil War and helping victims of Falangist oppression. In 1941, following the Nazi invasion of the Soviet Union, she established and headed the anti-fascist Junta de la Victoria which stood for democracy and women's suffrage. In 1965, Ratto founded Columna 10, a journal denouncing the conduct of the United States in the Vietnam War. In the 1970s, she published a series of important mathematics text books." , "lang" : "en" } ,      { "type" : "literal", "value" : "Corina Elo\u00EDsa Ratto conocida como Cora Ratto (Buenos Aires, 3 de enero de 1912-Barcelona, 2 de enero de 1981) fue una matem\u00E1tica, profesora y militante feminista y antifascista argentina, defensora de los derechos humanos y de los derechos de las mujeres.\u200B Fue cofundadora en 1941 de la , una organizaci\u00F3n feminista que agrup\u00F3 a miles de mujeres en todo el pa\u00EDs unidas para gestionar el env\u00EDo de ayuda para los aliados durante la Segunda Guerra Mundial y comprometidas en la defensa de la democracia." , "lang" : "es" } ] ,    "http://www.w3.org/2002/07/owl#differentFrom" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Cora_Sadosky" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Argentine_women's_rights_activists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:People_from_Buenos_Aires" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:University_of_Buenos_Aires_alumni" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1981_deaths" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Argentine_human_rights_activists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1912_births" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Women_human_rights_activists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:20th-century_Argentine_mathematicians" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Argentine_women_mathematicians" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Corina (Cora) Elo\u00EDsa Ratto de Sadosky (aka Cora Ratto, 1912\u20131981) was an Argentine mathematician, educator and militant activist in support of human and women's rights in Argentina and beyond. She played an important part in the Argentine University Federation supporting republican interests during the Spanish Civil War and helping victims of Falangist oppression. In 1941, following the Nazi invasion of the Soviet Union, she established and headed the anti-fascist Junta de la Victoria which stood for democracy and women's suffrage. In 1965, Ratto founded Columna 10, a journal denouncing the conduct of the United States in the Vietnam War. In the 1970s, she published a series of important mathematics text books." , "lang" : "en" } ,      { "type" : "literal", "value" : "Corina Elo\u00EDsa Ratto conocida como Cora Ratto (Buenos Aires, 3 de enero de 1912-Barcelona, 2 de enero de 1981) fue una matem\u00E1tica, profesora y militante feminista y antifascista argentina, defensora de los derechos humanos y de los derechos de las mujeres.\u200B Fue cofundadora en 1941 de la , una organizaci\u00F3n feminista que agrup\u00F3 a miles de mujeres en todo el pa\u00EDs unidas para gestionar el env\u00EDo de ayuda para los aliados durante la Segunda Guerra Mundial y comprometidas en la defensa de la democracia." , "lang" : "es" } ,      { "type" : "literal", "value" : "Corina Elo\u00EDsa Ratto (Buenos Aires, Argentina, 1912ko urtarrilaren 3a - 1981eko urtarrilaren 2a) matematikaria, giza eskubideen eta Argentinako emakumeen eskubideen aldeko aktibista izan zen." , "lang" : "eu" } ,      { "type" : "literal", "value" : "Cora Ratto de Sadosky n\u00E9e le 3 janvier 1912 en Argentine et morte le 2 janvier 1981 est une math\u00E9maticienne et militante des droits de l'homme et des droits des femmes argentine." , "lang" : "fr" } ,      { "type" : "literal", "value" : "Cora Eloisa Ratto de Sadosky (* 3. Januar 1912 in Argentinien; \u2020 2. Januar 1981 in Barcelona) war eine argentinische Mathematikerin, Hochschullehrerin und Menschenrechtsaktivistin." , "lang" : "de" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Argentines" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Buenos_Aires" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Argentine_women's_rights_activists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Activism" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Manuel_Sadosky" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/University_of_Buenos_Aires" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Spanish_Civil_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1981_deaths" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Falangist_Movement_of_Spain" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/\u00D1uSat" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Women's_suffrage" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/World_War_II" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Argentine_women_mathematicians" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:University_of_Buenos_Aires_alumni" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Mathematician" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Women's_rights" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Human_rights" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Argentine_human_rights_activi
{  "http://en.wikipedia.org/wiki/Peter_Kalischer" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Peter_Kalischer" } ] } ,  "http://dbpedia.org/resource/CBS_News" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Peter_Kalischer" } ] } ,  "http://dbpedia.org/resource/List_of_The_New_Yorker_contributors" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Peter_Kalischer" } ] } ,  "http://dbpedia.org/resource/Peter_Kalischer" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Journalist110224578" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/CausalAgent100007347" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatTheNewYorkerPeople" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/YagoLegalActorGeo" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/YagoLegalActor" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatAmericanJournalists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Object100002684" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Person100007846" } ,      { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Organism100004475" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Whole100003553" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/LivingThing100004258" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/Person" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Communicator109610660" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Writer110794014" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/PhysicalEntity100001930" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Peter Kalischer" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Peter Kalischer (December 25, 1915 \u2013 July 5, 1991) was an American journalist best known for his reporting of the early stages of the Vietnam War in the 1960s as a television correspondent for CBS News. He won the Overseas Press Club award in 1963 for his reporting during the Buddhist crisis that led to the fall of President Ngo Dinh Diem of South Vietnam. In 1968, while covering the Tet Offensive, he had dinner on the rooftop restaurant of the Caravelle Hotel with Walter Cronkite who was preparing a special report on the war and helped to convince him that the war could not be won militarily, that a stalemate was inevitable. From 1966 to 1978, Kalischer was the Paris correspondent and bureau chief for CBS News. He covered the Korean War, writing multiple articles about it." , "lang" : "en" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Journalists_from_California" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1991_deaths" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:The_New_Yorker_people" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:American_television_reporters_and_correspondents" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:American_war_correspondents_of_the_Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Loyola_University_New_Orleans_faculty" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1915_births" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:American_male_journalists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:20th-century_American_journalists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:American_war_correspondents" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Peter Kalischer (December 25, 1915 \u2013 July 5, 1991) was an American journalist best known for his reporting of the early stages of the Vietnam War in the 1960s as a television correspondent for CBS News. He won the Overseas Press Club award in 1963 for his reporting during the Buddhist crisis that led to the fall of President Ngo Dinh Diem of South Vietnam. In 1968, while covering the Tet Offensive, he had dinner on the rooftop restaurant of the Caravelle Hotel with Walter Cronkite who was preparing a special report on the war and helped to convince him that the war could not be won militarily, that a stalemate was inevitable. From 1966 to 1978, Kalischer was the Paris correspondent and bureau chief for CBS News. He covered the Korean War, writing multiple articles about it. Kalischer later became a professor of communications at Loyola University, a position he held until 1982." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:American_male_journalists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:American_television_reporters_and_correspondents" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/The_New_Yorker" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:The_New_Yorker_people" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Buddhist_crisis" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Loyola_University_New_Orleans_faculty" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:20th-century_American_journalists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Arre
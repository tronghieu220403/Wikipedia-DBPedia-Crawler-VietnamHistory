{  "http://en.wikipedia.org/wiki/Zolt\u00E1n_Szalkai" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Zolt\u00E1n_Szalkai" } ] } ,  "http://dbpedia.org/resource/Zolt\u00E1n_Szalkai" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/class/yago/YagoLegalActorGeo" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Whole100003553" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/YagoLegalActor" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/CausalAgent100007347" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Object100002684" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Creator109614315" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatHungarianFilmDirectors" } ,      { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/WikicatLivingPeople" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/LivingThing100004258" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/FilmDirector110088200" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Organism100004475" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/FilmMaker110088390" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Producer110480018" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Person100007846" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/PhysicalEntity100001930" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Zolt\u00E1n Szalkai" , "lang" : "eo" } ,      { "type" : "literal", "value" : "Zolt\u00E1n Szalkai" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Zolt\u00E1n Szalkai (born 1961) is a Hungarian documentary film director. He studied agriculture and geology before turning to a career in directing documentary films. One of the best known of these is On Foot through Gulag Land, an 8-part film series made on location in Russia. The film explores the many encounters Szalkai had when walking across Russia, as he met people who had been held in the forced labour camps and discussed their experiences with them. He was born and grew up in Hungary when it was dominated by the Soviet Union, and he was taught the Communist version of history." , "lang" : "en" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Hungarian_film_directors" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Living_people" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1961_births" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Gulag_in_literature_and_arts" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Zolt\u00E1n Szalkai (born 1961) is a Hungarian documentary film director. He studied agriculture and geology before turning to a career in directing documentary films. One of the best known of these is On Foot through Gulag Land, an 8-part film series made on location in Russia. The film explores the many encounters Szalkai had when walking across Russia, as he met people who had been held in the forced labour camps and discussed their experiences with them. He was born and grew up in Hungary when it was dominated by the Soviet Union, and he was taught the Communist version of history. Szalkai has also filmed in Vietnam and Indonesia. He filmed in Alaska, where he shot Wise Men of Alaska (2000) in the village of Wiseman. This was inspired by the American Robert Marshall's book Arctic Village (1933)." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Indonesia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Alaska" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Gulag" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Bob_Marshall_(wilderness_activist)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Gulag_in_literature_and_arts" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Communist" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Soviet_Union" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Living_people" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Hungarian_film_directors" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Hungary" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Arctic_Village_(book)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1961_births" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Kolyma" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Forced_labour" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Russia" } ] ,    "http://dbpedia.org/property/wikiPageUsesTemplate" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Short_description" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Authority_control" } ] ,    "http://dbpedia.org/ontology/wikiPageRevisionID" : [ { "type" : "literal", "value" : 1101596366 , "datatype" : "http://www.w3.org/2001/XMLSchema#integer" } ] ,    "http://dbpedia.org/ontology/wikiPageExternalLink" : [ { "type" : "uri", "value" : "http://www.dersu.hu" } ] ,    "http://dbpedia.org/ontology/wikiPageLength" : [ { "type" : "literal", "value" : "1293" , "datatype" : "http://www.w3.org/2001/XMLSchema#nonNegativeIn
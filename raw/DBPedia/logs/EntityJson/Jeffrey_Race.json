{  "http://en.wikipedia.org/wiki/Jeffrey_Race" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Jeffrey_Race" } ] } ,  "http://dbpedia.org/resource/Jeffrey_Race" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://dbpedia.org/ontology/Person" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Jeffrey Race" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Jeffrey Race is the author of War Comes to Long An, originally published on 1972 and re-published with additional material in 2010. The book is based on his interviews with Vietnamese and Vietnamese documents, both government and communist, in Long An province in what was then South Vietnam. Described as a \"military classic\", Race attempted to answer the question of why one side in the Vietnam War could better motivate its followers than the other. His conclusion was that by 1965, before the large scale introduction of American military forces into the war, the communist Viet Cong had already won the battle of \"Hearts and Minds\"." , "lang" : "en" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Psychological_warfare_theorists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:United_States_Army_soldiers" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Long_An_province" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:United_States_Army_personnel_of_the_Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Living_people" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Harvard_University_alumni" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Guerrilla_warfare_theorists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Year_of_birth_missing_(living_people)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Counterinsurgency_theorists" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Jeffrey Race is the author of War Comes to Long An, originally published on 1972 and re-published with additional material in 2010. The book is based on his interviews with Vietnamese and Vietnamese documents, both government and communist, in Long An province in what was then South Vietnam. Described as a \"military classic\", Race attempted to answer the question of why one side in the Vietnam War could better motivate its followers than the other. His conclusion was that by 1965, before the large scale introduction of American military forces into the war, the communist Viet Cong had already won the battle of \"Hearts and Minds\"." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Living_people" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:United_States_Army_personnel_of_the_Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Fairfield_County,_Connecticut" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Harvard_University" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Peasant" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Signal_Corps_(United_States_Army)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Counterinsurgency_theorists" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Crimestop" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Advanced_Research_Projects_Agency" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Communis
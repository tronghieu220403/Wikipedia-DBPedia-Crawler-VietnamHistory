ubject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_units_and_formations_of_South_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_units_and_formations_disestablished_in_1975" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "The 3rd Armored Cavalry Squadron (Vietnamese: Thi\u1EBFt \u0110o\u00E0n 3 Thi\u1EBFt gi\u00E1p) a battalion-sized unit of the Army of the Republic of Vietnam (ARVN), the South Vietnamese army. It was part of II Corps that oversaw the twelve provinces of the central highlands; corps headquarters being in the mountain town of Pleiku. The 3rd Armored Cav was organized on January 1, 1954. In 1971, the Presidential Unit Citation of the United States was awarded to the 3rd Armored Cavalry Squadron and attached U.S. Advisor/Liaison Personnel (MACV) for extraordinary heroism in action against an armed enemy during the period January 1, 1968, to September 30, 1968, in Pleiku and Binh Dinh Provinces. This makes the squadron one of only a few non-U.S. military units to receive the highest U.S. military honor awarded at the unit level." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/II_Corps_(South_Vietnam)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Tay_Nguyen" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/M113_armored_personnel_carrier" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Non-U.S._recipients_of_U.S._gallantry_awards" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_units_and_formations_of_South_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Pleiku" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/1968_in_the_Vietnam_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/File:South_Vietnam_Map
,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/MilitaryPerson" } ,      { "type" : "uri", "value" : "http://xmlns.com/foaf/0.1/Person" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Person100007846" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Alumnus109786338" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/CommissionedMilitaryOfficer109943239" } ,      { "type" : "uri", "value" : "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#NaturalPerson" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Wikicat\u00C9colePolytechniqueAlumni" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Scholar110557854" } ,      { "type" : "uri", "value" : "http://dbpedia.org/class/yago/Intellectual109621545" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Gustave Borgnis-Desbordes" , "lang" : "ca" } ,      { "type" : "literal", "value" : "Gustave Borgnis-Desbordes" , "lang" : "fr" } ,      { "type" : "literal", "value" : "Gustave Borgnis-Desbordes" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Gustave Borgnis-Desbordes, n\u00E9 \u00E0 Provins le 22 octobre 1839 et d\u00E9c\u00E9d\u00E9 \u00E0 Hano\u00EF le 18 juillet 1900, est un g\u00E9n\u00E9ral de division fran\u00E7ais. Il a particip\u00E9 \u00E0 la conqu\u00EAte coloniale fran\u00E7aise, notamment en Cochinchine et au Soudan fran\u00E7ais (il entre \u00E0 Bamako en 1883)." , "lang" : "fr" } ,      { "type" : "literal", "value" : "Gustave Borgnis-Desbordes fou un militar franc\u00E8s nascut a Provins el 22 d'octubre de 1839 i nort a Hanoi el 18 de juliol de 1900. Va participar en la colonitzaci\u00F3 francesa especialment al Territori de l'Alt Senegal i a Indoxina. \n* Primera campanya de Borgnis-Desbordes \n* Segona campanya de Borgnis-Desbordes \n* Tercera campanya de Borgnis-Desbordes" , "lang" : "ca" } ,      { "type" : "literal", "value" : "Gustave Borgnis-Desbordes (22 October 1839 \u2013 18 July 1900) was a French general. He was a major figure in the French Imperial conquest of the French Sudan, modern Mali. He was Commandant-Superior of the French Military Territory of (French territory of Upper Senegal), attached to the colonial government of Senegal, 1880 to 1883." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "Gustave Borgnis-Desbordes" , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/depiction" : [ { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Gustave_Borgnis-Desbordes_Appert_BNF_Gallica.jpg" } ,      { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Fortdekita.jpg" } ,      { "type" : "uri", "value" : "http://commons.wikimedia.org/wiki/Special:FilePath/Flag_of_France.svg" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:French_colonisation_in_Africa" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Colonial_Governors_of_French_Mali" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:French_generals" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:People_from_Provins" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:French_military_personnel_of_the_Sino-French_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:People_of_French_Indochina" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:French_colonial_governors_and_administrators" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:People_of_French_West_Africa" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Burials_at_the_Cemetery_of_Saint-Louis,_Versailles" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1900_deaths" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1839_births" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:\u00C9cole_Polytechnique_alumni" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Gustave Borgnis-Desbordes fou un militar franc\u00E8s nascut a Provins el 22 d'octubre de 1839 i nort a Hanoi el 18 de juliol de 1900. Va participar en la colonitzaci\u00F3 francesa especialment al Territori de l'Alt Senegal i a Indoxina. La seva fam\u00EDlia era de Morlaix i era net d'un diputat de Finist\u00E8re i fill d'un polit\u00E8cnic; ell mateix va entrar a l'escola polit\u00E8cnica el 1859 i va seguir despr\u00E9s els cursos de l'escola d'artilleria de Metz de 1861 a 1863, i despr\u00E9s va entrar a l'artilleria de marina. Fou tinent el 1863 i va servir a Toulon fins al 1866. L'any seg\u00FCent fou ascendit a capit\u00E0. Va servir a Cotxinxina de 1868 a 1871; despr\u00E9s va servir a Par\u00EDs com adjunt del general Fr\u00E9bault, inspector general permanent de l'artilleria de marina. Fou cap d'esquadr\u00F3 el 1876 i ascendit a tinent coronel el 1880. Fou llavors enviat al Senegal com a comandant superior del territori de l'Alt Senegal, que va dirigir tres anys en els quals va fer tres campanyes: \n* Primera campanya de Borgnis-Desbordes \n* Segona campanya de Borgnis-Desbordes \n* Tercera campanya de Borgnis-Desbordes El 31 de mar\u00E7 de 1882 fou ascendit a coronel i va combatre despr\u00E9s al Tonkin (1884-1886 i despr\u00E9s 1888-1889) on va manar l'artilleria del cos expedicionari i va dirigir els combats de Cho-Mo\u00EF i Cho-Chu. Fou nomenat general de brigada el 25 de juliol de 1886, i general de divisi\u00F3 el 24 de mar\u00E7 de 1890. Fou despr\u00E9s comandant en cap de les tropes franceses a Indoxina, posici\u00F3 en la qual va morir. Fou tamb\u00E9 Gran oficial de la Legi\u00F3 d'Honor." , "lang" : "ca" } ,      { "type" : "literal", "value" : "Gustave Borgnis-Desbordes (22 October 1839 \u2013 18 July 1900) was a French general. He was a major figure in the French Imperial conquest of the French Sudan, modern Mali. He was Commandant-Superior of the French Military Territory of (French territory of Upper Senegal), attached to the colonial government of Senegal, 1880 to 1883." , "lang" : "en" } ,      { "type" : "literal", "value" : "Gustave Borgnis-Desbordes, n\u00E9 \u00E0 Provins le 22 octobre 1839 et d\u00E9c\u00E9d\u00E9 \u00E0 Hano\u00EF le 18 juillet 1900, est un g\u00E9n\u00E9ral de division fran\u00E7ais. Il a particip\u00E9 \u00E0 la conqu\u00EAte coloniale fran\u00E7aise, notamment en Cochinchine et au Soudan fran\u00E7ais (il entre \u00E0 Bamako en 1883)." , "lang" : "fr" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Imamate_of_Futa_Toro" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/French_Indochina" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Versailles_(city)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Tukulor_Empire" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Senegal" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Saboucir\u00E9" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Murgala" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Ren\u00E9_Servatius" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Haut-S\u00E9n\u00E9gal" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Keni\u00E9ra" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Beledougou" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Yellow_fever" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Daba_(Bambara_Empire)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/File:Fortdekita.jpg" } ,      { "type" : "uri", "value" 
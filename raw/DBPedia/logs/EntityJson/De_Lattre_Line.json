{  "http://en.wikipedia.org/wiki/De_Lattre_Line" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/De_Lattre_Line" } ] } ,  "http://dbpedia.org/resource/First_Indochina_War" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/De_Lattre_Line" } ] } ,  "http://dbpedia.org/resource/McNamara_Line" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/De_Lattre_Line" } ] } ,  "http://dbpedia.org/resource/320th_Division_(Vietnam)" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/De_Lattre_Line" } ] } ,  "http://dbpedia.org/resource/Battle_of_Ngh\u0129a_L\u1ED9_(1952)" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/De_Lattre_Line" } ] } ,  "http://dbpedia.org/resource/De_Lattre_Line" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/MilitaryStructure" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/ArchitecturalStructure" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "De Lattre Line" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "The De Lattre Line, named after General Jean de Lattre de Tassigny, was a line of concrete fortifications, obstacles, and weapons installations constructed by the French around the Red River Delta in northern Vietnam. The French established the fortification to guard the essential lines of communication between the cities of Hanoi and Haiphong and to provide security for the densely populated and economically important Delta area against attacks by both the Vi\u1EC7t Minh and any potential invasion from Communist China." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "De Lattre Line" , "lang" : "en" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Fortifications_in_Vietnam" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "The De Lattre Line, named after General Jean de Lattre de Tassigny, was a line of concrete fortifications, obstacles, and weapons installations constructed by the French around the Red River Delta in northern Vietnam. The French established the fortification to guard the essential lines of communication between the cities of Hanoi and Haiphong and to provide security for the densely populated and economically important Delta area against attacks by both the Vi\u1EC7t Minh and any potential invasion from Communist China. The Line was to comprise 1200 separate concrete blockhouses able to withstand 155mm artillery grouped in 250 clusters of 3-6 blockhouses for mutual fire support over a span of 235 miles (378 km). Each blockhouse was to hold a minimum of 10 men. In addition a defensive redoubt was to be constructed around a 22 miles (35 km) radius from the port of Haiphong ensuring its safety from artillery attack. All these new defensive lines were to be connected by roads capable of bearing 30-ton tanks. Construction commenced in late 1950 and was largely complete by the end of 1951. While the De Lattre Line provided some degree of security and formed an anchor point for French mobile operations outside the Delta, it was far from impenetrable and entire Vi\u1EC7t Minh combat units could be easily infiltrated through the gaps between strongpoints. Following the withdrawal of the French from northern Vietnam in October 1954 the De Lattre Line was abandoned. Today the deteriorating blockhouses are either unused or used by farmers." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/First_Indochina_War" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/McNamara_Line" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Battle_of_V\u0129nh_Y\u00EAn" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Battle_of_M\u1EA1o_Kh\u00EA" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vi\u1EC7t_Minh" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Battle_of_the_Day_River" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Fortification" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Red_River_Delta" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Maginot_Line" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Hanoi" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Fortifications_in_Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Jean_de_Lattre_de_Tassigny" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Haiphong" } ] ,    "http://dbpedia.org/property/wikiPageUsesTemplate" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:VietnamWar-stub" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Rp" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Convert" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Coord_missing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Infobox_military_structure" } ] ,    "http://dbpedia.org/property/location" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Red_River_Delta" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam" } ] ,    "http://dbpedia.org/ontology/wikiPageRevisionID" : [ { "type" : "literal", "value" : 1
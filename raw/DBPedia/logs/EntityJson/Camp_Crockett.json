{  "http://en.wikipedia.org/wiki/Camp_Crockett" : { "http://xmlns.com/foaf/0.1/primaryTopic" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Camp_Crockett" } ] } ,  "http://dbpedia.org/resource/Fort_Gordon" : { "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Camp_Crockett" } ] } ,  "http://dbpedia.org/resource/Camp_Crockett" : { "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ { "type" : "uri", "value" : "http://www.w3.org/2002/07/owl#Thing" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/ArchitecturalStructure" } ,      { "type" : "uri", "value" : "http://dbpedia.org/ontology/MilitaryStructure" } ] ,    "http://www.w3.org/2000/01/rdf-schema#label" : [ { "type" : "literal", "value" : "Camp Crockett" , "lang" : "en" } ] ,    "http://www.w3.org/2000/01/rdf-schema#comment" : [ { "type" : "literal", "value" : "Camp Crockett was established in late 1967 on the Fort Gordon, Georgia, federal reservation for the training of soldiers preparatory to Airborne and Special Forces schools during the Vietnam war. Located on an isolated part of the installation, the camp included a mockup of a village set up to look like one that would be encountered in Vietnam. Soldiers training at the camp were housed in Quonset huts, one of which served as a field kitchen." , "lang" : "en" } ] ,    "http://xmlns.com/foaf/0.1/name" : [ { "type" : "literal", "value" : "Camp Crockett" , "lang" : "en" } ] ,    "http://purl.org/dc/terms/subject" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buildings_and_structures_in_Richmond_County,_Georgia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buildings_and_structures_in_Jefferson_County,_Georgia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buildings_and_structures_in_McDuffie_County,_Georgia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1967_establishments_in_Georgia_(U.S._state)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_installations_established_in_1967" } ] ,    "http://dbpedia.org/ontology/abstract" : [ { "type" : "literal", "value" : "Camp Crockett was established in late 1967 on the Fort Gordon, Georgia, federal reservation for the training of soldiers preparatory to Airborne and Special Forces schools during the Vietnam war. Located on an isolated part of the installation, the camp included a mockup of a village set up to look like one that would be encountered in Vietnam. Soldiers training at the camp were housed in Quonset huts, one of which served as a field kitchen. Abandoned in 1969, the locations of the Quonset huts are marked by concrete slabs in a pine forest planted as part of a reforestation project instituted in 1970. In 2010, news reports indicated that Agent Orange herbicide was applied by helicopter to 98 acres of Training Area 47 in July 1967 prior to the establishment of Camp Crockett." , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageWikiLink" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/United_States_Army_Airborne_School" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Agent_Orange" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam_war" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Military_installations_established_in_1967" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Vietnam" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Special_Forces_(United_States_Army)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Helicopter" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:1967_establishments_in_Georgia_(U.S._state)" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/US_Army" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Military_base" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Reforestation" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buildings_and_structures_in_Richmond_County,_Georgia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buildings_and_structures_in_Jefferson_County,_Georgia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Field_kitchen" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Village" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Concrete" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Category:Buildings_and_structures_in_McDuffie_County,_Georgia" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Quonset_hut" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Fort_Gordon" } ] ,    "http://dbpedia.org/property/wikiPageUsesTemplate" : [ { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Reflist" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Army" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Infobox_Military_Structure" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Short_description" } ,      { "type" : "uri", "value" : "http://dbpedia.org/resource/Template:Coord_missing" } ] ,    "http://dbpedia.org/property/location" : [ { "type" : "literal", "value" : "Southwest of Augusta, Georgia" , "lang" : "en" } ] ,    "http://dbpedia.org/ontology/wikiPageRevisionID" : [ { "type" : "literal", "value" : 954924805 , "datatype" : "http://www.w3.org/2001/XMLSchema#integer" } ] ,    "http://dbpedia.org/ontology/wikiPageExternalLink" : [ { "type" : "uri", "value" : "http://campcrockett.blogspot.com/" } ,      { "type" : "uri", "value" : "http://taipeisignalarmy.blogspot.com/2014/11/a-camp-within-fort-camp-crocket-georgia.html" } ] ,    "http://dbpedia.org/ontology/wikiPageLength" : [ { "type" : "literal", "
i sinh":[{"qualifiers":{"quốc gia":[{"id":"Q58296","type":"wikibase-item","value":"Đệ nhất Cộng hòa Pháp"}]},"id":"Q6441","type":"wikibase-item","value":"Montpellier"}],"ngôn ngữ mẹ đẻ":[{"id":"Q150","type":"wikibase-item","value":"tiếng Pháp"}],"ngày sinh":[{"type":"string","value":"năm 1798"}],"tên":[{"type":"string","value":"Auguste"}],"lĩnh vực làm việc":[{"id":"Q5891","type":"wikibase-item","value":"triết học"},{"type":"string","value":"xã hội học"}],"thể loại ở Commons":[{"type":"string","value":"Auguste Comte"}],"nơi chôn cất":[{"qualifiers":{"":[{"type":"string","value":"17"}],"thể loại ở Commons":[{"type":"string","value":"Grave of Auguste Comte"}]},"id":"Q311","type":"wikibase-item","value":"Nghĩa trang Père-Lachaise"}]},"description":"triết gia người Pháp","id":"Q12718","label":"Auguste Comte"}